#include <stdio.h>
 
int main() {
 
    
    while(1){
        int h1, h2, m1, m2;
        scanf("%d %d %d %d", &h1, &m1, &h2, &m2);
        if(h1==0 && h2==0 && m1 == 0 && m2 ==0){
            break;
        }
        int minutosAgora = h1*60+m1;
        int minutosAlarme = h2*60+m2;
        
        if(minutosAlarme>minutosAgora){
            printf("%d\n", minutosAlarme-minutosAgora);
        }else if(minutosAlarme<=minutosAgora){
            printf("%d\n", 24*60+minutosAlarme-minutosAgora);
        }
    
    }
    
    
    return 0;
}
