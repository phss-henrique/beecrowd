#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void) {
    char *str = malloc(1001*sizeof(char)); 
    char front = '(';
    char back = ')';
    
    while (scanf("%s", str) != EOF) {
        int aberto = 0, fechado = 0;
        
        for (int i = 0; str[i] != '\0'; i++) {
            if (str[i] == front) {
                aberto++;
            } else if (str[i] == back) {
                fechado++;
            }
            if (fechado > aberto) {
                printf("incorrect\n");
                break;
            }
        }
        
        if (aberto == fechado) {
            printf("correct\n");
        } else if(aberto>fechado){
            printf("incorrect\n");
        }
    }
    free(str);
    return 0;
}
